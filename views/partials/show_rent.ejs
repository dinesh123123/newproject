<style>#line_chart {  
    height: 1px;
     margin-top: opx;
     margin-left: 25px; }
     </style>

      <section class="content dashboard">
            <div class="main-graph">
                    <div class="row clearfix">
                        <div class="col-xs-12 col-sm-6 col-md-8 col-lg-10 padding-0 overflow-hidden">
                            <div class="graph-area">
                                 <div id="line_chart"></div>
                            </div> 
                        </div>
                      
                    </div>
                </div>
                <!-- #END# Main Graph-->
            <div class="page-heading">
                <h1> Rent Orders</h1>
                <ol class="breadcrumb">
                    <li><a href="/public/index">Home</a></li>
                    <li class="active">Rent orders</li>
                </ol>
            </div>


            <div class="panel panel-default">
            
           <div class="panel-heading"></div>
           
                    <div class="panel-body">
                        <div class="table-responsive">
                            <table id="myTable" class="table js-exportable">
                            
                            <thead>
                                    <tr>
                                        <th>S.No</th>
                                        <th>TransectionId</th>
                                        <th>Address</th>                                    
                                        <th>City</th>
                                        <th>Pincode</th>
                                        <th>Amount</th>
                                        <th>PaymentMode</th>
                                        <th>PaymentStatus</th>
                                        <th>PaymentDate</th>
                                        <th>Status</th>
                                        <th>Edit</th>
                                        <th>Detail</th>
                                       
                                    </tr>
                                </thead>
                                <tbody>
            <% for(var i=0; i < data.length; i++) { %>
                                   
        <tr>
         <td><%= i+1%></td>
         <td><%=data[i]._id%></td>
 <td><%=data[i].ShippingAddressId.address%></td>
 <td><%=data[i].ShippingAddressId.city%></td>
 <td><%=data[i].ShippingAddressId.pincode%></td>
 <td><b>Rs</b> <%=data[i].t_price%></td>
 <td><%=data[i].paymentMode%></td>
 <td><%=data[i].isPaid%></td>
 <td><%=data[i].order_date%></td>
 <td><%=data[i].status%></td>
 <td><a href="/public/rentorders_update/<%=data[i].id%>" class="btn btn-primary"><i class="fa fa-edit">Edit</i></a></td>
 <td><a href="/public/rentorder_details/<%=data[i].id%>" class="btn btn-primary">view</a></td>
                                        </tr>
                                        <% } %>
                                     
                               </tbody> 
                        
                            </table>
                        </div>
                    </div>
                </div>
                <!-- #END# Task data -->
            </div>
        </section>